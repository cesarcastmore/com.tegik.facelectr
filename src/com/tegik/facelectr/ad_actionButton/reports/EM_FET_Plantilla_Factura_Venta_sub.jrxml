	<queryString>
		<![CDATA[SELECT pro.value clave,
       il.Qtyinvoiced Cantidad,
       uom.name medida,
       COALESCE(pro.Name,g.name) AS Nombre,
       COALESCE(pro.description,'Sin descripción') as pdescripcion,
       CASE
       WHEN COALESCE(tax.em_fet_isieps,'N') = 'N' OR (COALESCE(tax.em_fet_isieps,'N') = 'Y' AND COALESCE(bp.em_fet_desglosarieps,'N') = 'Y') OR (COALESCE(tax.em_fet_isieps,'N') = 'Y' AND COALESCE(bp.em_fet_desglosarieps,'N') = 'N' AND COALESCE(il.qtyinvoiced,0) = 0) then COALESCE(il.PriceActual,0.00)
       WHEN COALESCE(tax.em_fet_isieps,'N') = 'Y' AND COALESCE(bp.em_fet_desglosarieps,'N') = 'N' AND COALESCE(il.qtyinvoiced,0) <> 0 THEN ((COALESCE(il.LineNetAmt,0.00) + COALESCE(TAX.taxamt,0.00)) / qtyinvoiced)
       END Precio,
       CASE
       WHEN COALESCE(tax.em_fet_isieps,'N') = 'N' OR (COALESCE(tax.em_fet_isieps,'N') = 'Y' AND COALESCE(bp.em_fet_desglosarieps,'N') = 'Y') OR (COALESCE(tax.em_fet_isieps,'N') = 'Y' AND COALESCE(bp.em_fet_desglosarieps,'N') = 'N' AND COALESCE(il.qtyinvoiced,0) = 0) then COALESCE(il.LineNetAmt,0.00)
       WHEN COALESCE(tax.em_fet_isieps,'N') = 'Y' AND COALESCE(bp.em_fet_desglosarieps,'N') = 'N' AND COALESCE(il.qtyinvoiced,0) <> 0 THEN COALESCE(il.LineNetAmt,0.00) + COALESCE(TAX.taxamt,0.00)
       END Importe,
       COALESCE(il.description,'') Descripcion
       FROM C_invoiceLine il
  INNER JOIN C_invoice i ON i.C_invoice_ID = il.C_invoice_ID
  INNER join c_uom uom on il.c_uom_id = uom.c_uom_id
  INNER JOIN c_bpartner bp ON i.c_bpartner_id = bp.c_bpartner_id
  LEFT JOIN M_Product pro ON pro.M_Product_ID = il.M_Product_ID
  LEFT JOIN c_glitem g ON il.account_id = g.c_glitem_id
  LEFT JOIN (SELECT c_invoiceline_id,
       SUM(COALESCE(Taxamt,0)) taxamt,
       tax.em_fet_isieps em_fet_isieps
FROM c_invoicelinetax ilt
  INNER JOIN c_tax tax
          ON ilt.c_tax_id = tax.c_tax_id
         AND COALESCE(tax.em_fet_isieps,'N') = 'Y'
         GROUP BY c_invoiceline_id, tax.em_fet_isieps) tax
 on il.c_invoiceline_id = tax.c_invoiceline_id
WHERE il.C_invoice_ID = $P{DOCUMENT_ID}
ORDER BY il.line]]>
	</queryString>
