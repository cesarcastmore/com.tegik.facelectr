<SqlClass name="LlamarConsultas" package="com.tegik.facelectr.ad_actionButton">
  <SqlMethod name="isDesglosarIEPS" type="preparedStatement" return="String">
    <Sql>
      SELECT Em_Fet_DesglosarIEPS AS isdeglosar 
         FROM C_Bpartner
           WHERE C_Bpartner_ID= ? 
